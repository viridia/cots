{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "command": "run",
      "args": ["--example", "terrain"],
      "problemMatcher": ["$rustc"],
      "label": "rust: cargo run --example terrain",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "env": {
        "RUST_BACKTRACE": "1"
      }
    }
  ]
}
